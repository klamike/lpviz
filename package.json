{
  "name": "lpviz",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "format": "prettier --write --print-width 3000 lpviz && bun lpviz/restoreAsciiArt.ts && bunx tsc && bunx knip",
    "deploy": "npx wrangler deploy --assets=./dist --name lpviz --compatibility-date 2025-09-01",
    "prdeploy": "npx wrangler versions upload --assets=./dist --name lpviz --compatibility-date 2025-09-01"
  },
  "devDependencies": {
    "@types/three": "^0.181.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.0",
    "@tanstack/virtual-core": "^3.13.12",
    "jsoncrush": "^1.1.8",
    "ml-matrix": "^6.12.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sprintf-js": "^1.1.3",
    "three": "^0.181.1",
    "wrangler": "^4.47.0"
  },
  "knip": {
    "entry": [
      "lpviz/src/main.ts"
    ],
    "project": [
      "lpviz/src/**/*.ts"
    ]
  }
}

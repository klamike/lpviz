{
  "name": "lpviz",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite lpviz --port 3000",
    "build:lpviz": "vite build lpviz --outDir ../dist --emptyOutDir",
    "build:docs": "vite build lpviz/docs --outDir ../../dist/docs",
    "build": "vite build lpviz --outDir ../dist --emptyOutDir && vite build lpviz/docs --outDir ../../dist/docs",
    "preview": "vite preview lpviz --port 3000 --outDir ../dist"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "jsoncrush": "^1.1.8",
    "ml-matrix": "^6.12.1",
    "sprintf-js": "^1.1.3"
  }
}
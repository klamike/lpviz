{
  "name": "lpviz",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "format": "prettier --write --print-width 3000 lpviz && bun lpviz/restoreAsciiArt.ts",
    "deploy": "npx wrangler deploy --assets=./dist --name lpviz --compatibility-date 2025-09-01",
    "prdeploy": "npx wrangler versions upload --assets=./dist --name lpviz --compatibility-date 2025-09-01"
  },
  "devDependencies": {
    "@types/three": "^0.181.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "jsoncrush": "^1.1.8",
    "ml-matrix": "^6.12.1",
    "sprintf-js": "^1.1.3",
    "three": "^0.181.1"
  }
}
